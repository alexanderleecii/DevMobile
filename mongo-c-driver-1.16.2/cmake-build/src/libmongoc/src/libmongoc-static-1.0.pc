prefix=/usr/local
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${exec_prefix}/include

Name: libmongoc
Description: The libmongoc MongoDB client library.
Version: 1.16.2
Requires: libbson-static-1.0
Libs: -L${libdir} -lmongoc-static-1.0  -L/usr/lib -lsasl2 -framework CoreFoundation -framework Security -lresolv -L/usr/lib -lz
Cflags: -I${includedir}/libmongoc-1.0 -DMONGOC_STATIC
